{
  "entrypoint": "index.html",
  "shell": "src/home-ui-app/home-ui-app.js",
  "sources": [
    "src/**/*",
    "package.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "node_modules/@webcomponents/webcomponentsjs/**/*",
    "node_modules/web-animations-js/**/*"
  ],
  "builds": [
    {
      "name": "es6-unbundled",
      "js": {"minify": true},
      "css": {"minify": true},
      "html": {"minify": true},
      "addServiceWorker": true,
      "addPushManifest": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}